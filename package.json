{
  "name": "car-price-app",
  "version": "1.0.0",
  "description": "Car price application with backend and frontend",
  "private": true,
  "workspaces": [
    "backend",
    "client"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=backend\" \"npm run start --workspace=client\"",
    "build": "npm run build --workspace=client",
    "start": "npm run start --workspace=backend"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
